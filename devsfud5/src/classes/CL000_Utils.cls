	/*Get month spanish names form specific date time*/
	public static string GetMonthName(DateTime DateToEval){

		string oRetorno = '';
		
		integer intMont = DateToEval.month();

		if(intMont == 1) oRetorno = '1. Enero';
		else if(intMont == 2) oRetorno = '2. Febrero';
		else if(intMont == 3) oRetorno = '3. Marzo';
		else if(intMont == 4) oRetorno = '4. Abril';
		else if(intMont == 5) oRetorno = '5. Mayo';
		else if(intMont == 6) oRetorno = '6. Junio';
		else if(intMont == 7) oRetorno = '7. Julio';
		else if(intMont == 8) oRetorno = '8. Agosto';
		else if(intMont == 9) oRetorno = '9. Septiembre';
		else if(intMont == 10) oRetorno = '10. Octubre';
		else if(intMont == 11) oRetorno = '11. Noviembre';
		else if(intMont == 12) oRetorno = '12. Diciembre';
		
		return oRetorno;
	}
	
	/*Standar token validation*/
	public static Boolean ValidateStandarTokenFormat(string Token){
		
		Boolean oRetorno = true; 
		
		if(Token == null || Token.length() != 8){
			oRetorno = false;
		}
		else{
			for(Integer i=0;i<Token.length();i++){
				if(!C_ValidTokenChars.contains(Token.mid(i,1))){
					oRetorno = false;
					break;
				}
			}
		}
		return oRetorno;		
	}
	
